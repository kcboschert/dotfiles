{
    "Godot custom node template": {
        "prefix": "gnode",
        "body": [
            "using Godot;",
            "",
            "public partial class ${1:$TM_FILENAME_BASE} : ${2:Node} {",
            "    public override void _Ready() {",
            "        base._Ready();",
            "    }",
            "",
            "    public override void _Process(double delta) {",
            "        base._Process(delta);",
            "    }",
            "}"
        ],
        "description": "Print to Godot console"
    },

    "Godot print": {
        "prefix": "gprint",
        "body": "GD.Print($1);",
        "description": "Print to Godot console"
    },

    "Godot _EnterTree()": {
        "prefix": "gentertree",
        "body": [
            "public override void _EnterTree() {",
            "    base._EnterTree();$1",
            "}"
        ],
        "description": "Godot _EnterTree() virtual method"
    },
