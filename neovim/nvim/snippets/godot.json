    "GDUnit4 TestSuite": {
        "prefix": "testsuite",
        "body": [
          "using GdUnit4;",
          "using Godot;",
          "using static GdUnit4.Assertions;",

          "[TestSuite]",
          "[RequireGodotRuntime]",
          "public class ${1:$TM_FILENAME_BASE}",
          "{",
          "    $2",
          "}"
        ],
        "description": "GDUnit4 Test Suite Class"
    },
    "GDUnit4 TestCase": {
        "prefix": "testcase",
        "body": [
          "[TestCase]",
          "public void ${1:ItDoesSomething}()",
          "{",
          "    AssertThat(true).IsTrue();$2",
          "}"
        ],
        "description": "GDUnit4 Test Case"
    }
