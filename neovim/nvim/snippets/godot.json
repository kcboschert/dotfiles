    "Godot [Export]": {
        "prefix": "export",
        "body": "[Export] ${1:private} ${2:Node} ${3:Type};",
        "description": "Godot [Export] attribute"
    },
    "Godot [ExportGroup]": {
        "prefix": "exportg",
        "body": "[ExportGroup(\"$1\")]",
